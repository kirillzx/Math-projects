Cell[BoxData[
 RowBox[{
  RowBox[{"\[Omega]", "[", 
   RowBox[{"n_", ",", "f_", ",", "list_"}], "]"}], ":=", 
  RowBox[{"Sum", "[", 
   RowBox[{
    FractionBox[
     RowBox[{"f", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
     RowBox[{"Product", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"k", "\[NotEqual]", " ", "i"}], ",", 
         RowBox[{
          RowBox[{
          "list", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], "-", 
          RowBox[{
          "list", "\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}]}], 
         ",", "1"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]], ",", 
    RowBox[{"{", 
